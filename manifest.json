{
  "name": "Open Tabs notify",
  "version": "1.0",
  "description": "Informs about increasing number of tabs if there are more than ten",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage",
    "cookies",  
    "system.display"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  }
}